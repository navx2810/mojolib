{
    "compilerOptions": {
        "outDir": "./lib/",
        "sourceMap": true,
        "strict": true,
        "lib": ["es2018", "dom"],
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "declaration": true,
        "moduleResolution": "node",
        "target": "es5",
    },
    "include": [
        "./src/**/*.ts"
    ],
    "exclude": [
        "./src/web.ts"
    ]
}